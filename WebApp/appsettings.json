{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "[PLACEHOLDER_TENANT_ID]",
    "ClientId": "[PLACEHOLDER_CLIENT_ID]",
    "ClientSecret": "[PLACEHOLDER_CLIENT_SECRET]",
    "CallbackPath": "/signin-oidc"
  },
  "DownstreamApis": {
    "APINoGraph": {
      "BaseUrl": "https://localhost:7067",
      "Scopes": "api://[PLACEHOLDER_API_NO_GRAPH_CLIENT_ID]/.default"
    },
    "APIWithGraph": {
      "BaseUrl": "https://localhost:7087",
      "Scopes": "api://[PLACEHOLDER_API_WITH_GRAPH_CLIENT_ID]/access_as_user"
    }
  },
  "APIEndpoints": {
    "APINoGraphBaseUrl": "https://localhost:7067",
    "APIWithGraphBaseUrl": "https://localhost:7087"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
